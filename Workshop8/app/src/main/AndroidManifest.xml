<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="oosd.team1.workshop8">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity_bookingedit" android:parentActivityName=".activity_bookingslist" android:windowSoftInputMode="adjustResize"></activity>
        <activity
            android:name=".activity_addBooking"
            android:parentActivityName=".activity_bookingslist"
            android:windowSoftInputMode="adjustResize" />
        <activity android:name=".CustomerActivity" android:parentActivityName=".MainActivity">
        </activity>
        <activity android:name=".CustomerDetail" android:parentActivityName=".CustomerActivity">
        </activity>
        <activity
            android:name=".activity_bookingslist"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".AgentDetail"
            android:parentActivityName=".AgentActivity" />
        <activity android:name=".ExtraActivity" />

        <activity
            android:name=".AgentActivity"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>